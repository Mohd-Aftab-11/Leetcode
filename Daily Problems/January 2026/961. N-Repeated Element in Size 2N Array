class Solution {
    public int repeatedNTimes(int[] nums) {
        for (int i = 0; i < nums.length; i++) {
            for (int j = 1; j <= 3; j++) {
                if (i + j < nums.length && nums[i] == nums[i + j]) {
                    return nums[i];
                }
            }
        }
        
        return -1;
    }
}
